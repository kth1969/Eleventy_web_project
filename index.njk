---
  
<div id="first">
<div class="sticky-timeline">
<h2 id="time" class="text-rotate-2">1997</h2>
 </div>
<section class="wrapper-grid">





{% for screenshot in collection_1 %}

{% set busyClass = "" %}
  {% if screenshot.busy == true %}
  {% set busyClass = "busy" %}
 
{% endif %}
<div class="image-span-wrapper">
   <img class="{{busyClass}}"src="/img/{{screenshot.name}}"> 
</div>
 {% endfor %} 

</section>
</div>

<div id="second">
<div class="changetime"><p class="changetimetext">2001</p></div>
<div class="sticky-timeline">
  <h2 id="timetwo"class="text-rotate-2">2001</h2> 
   </div>

<section class="wrapper-grid-2">

  

{% for screenshot in collection_2 %}

{% set boldClass = "" %}
  {% if screenshot.bold_colors == true %}
  {% set boldClass = "bold" %}
 
{% endif %}
<div class="image-wrapper-2">
   <img class="{{boldClass}}"src="/img/{{screenshot.name}} "> 
 
</div>
 {% endfor %} 
 
</section>

</div>
